        "pako": "^2.0.4",
        "prettier": "^2.5.1",
        "pretty-bytes": "^6.0.0",
        "solid-js": "^1.3.7",
        "typescript": "^4.5.5",
        "web-animations-js": "^2.3.2",
        "workbox-window": "^6.4.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@csstools/postcss-sass": "^5.0.0",
        "@types/node": "^17.0.18",
        "autoprefixer": "^10.4.2",
        "browser-sync": "^2.27.7",
        "commitizen": "^4.2.4",
        "del": "^6.0.0",
        "esbuild": "^0.14.22",
        "esbuild-plugin-solid": "^0.4.2",
        "gulp": "^4.0.2",
        "gulp-esbuild": "^0.10.1",
        "gulp-if": "^3.0.0",
        "gulp-plumber": "^1.2.1",
