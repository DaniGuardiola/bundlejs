{
    "name": "bundle",
    "version": "0.0.5",
    "private": true,
    "license": "MIT",
    "type": "module",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "commit": "git cz",
        "build": "gulp build",
        "watch": "gulp --watch",
        "test": "pnpm build",
        "release": "standard-version && git add -A && git commit -m 'chore(release): :rocket:'"
    },
    "changelog": {
        "repo": "bundle",
        "labels": {
            "breaking": ":boom: Breaking Change",
            "enhancement": ":rocket: Enhancement",
            "bug": ":bug: Bug Fix",
            "documentation": ":memo: Documentation",
            "internal": ":house: Internal"
        },
        "cacheDir": ".changelog"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
        }
    },
    "dependencies": {
        "@amoutonbrady/lz-string": "^0.0.1",
