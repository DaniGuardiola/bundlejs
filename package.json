{
    "name": "bundle",
    "version": "0.0.2",
    "private": true,
    "license": "MIT",
    "type": "module",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "commit": "git cz",
        "build": "gulp build",
        "watch": "gulp --watch",
        "test": "pnpm build",
        "release": "standard-version && git add -A && git commit -m 'chore(release): :rocket:' && git push --follow-tags"
    },
    "changelog": {
        "repo": "bundle",
        "labels": {
            "breaking": ":boom: Breaking Change",
            "enhancement": ":rocket: Enhancement",
            "bug": ":bug: Bug Fix",
            "documentation": ":memo: Documentation",
            "internal": ":house: Internal"
        },
        "cacheDir": ".changelog"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
        }
    },
    "dependencies": {
        "@amoutonbrady/lz-string": "^0.0.1",
        "@okikio/animate": "^2.4.0",
        "@okikio/emitter": "^2.1.7",
        "@okikio/native": "^3.0.1",
        "assert": "^2.0.0",
        "browser-builtins": "^3.3.1",
        "buffer": "^6.0.3",
        "countapi-js": "^1.0.2",
        "esbuild-wasm": "^0.12.28",
        "events": "^3.3.0",
        "highlight.js": "^11.2.0",
        "memfs": "^3.3.0",
        "monaco-editor": "^0.27.0",
        "pako": "^2.0.4",
        "path": "^0.12.7",
        "prettier": "^2.4.1",
        "pretty-bytes": "^5.6.0",
        "rollup-plugin-virtual-fs": "^4.0.1-alpha.0",
        "solid-js": "^1.1.4",
        "stream": "^0.0.2",
        "typescript": "^4.4.3",
        "web-animations-js": "^2.3.2",
        "workbox-window": "^6.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@csstools/postcss-sass": "^4.0.0",
        "@types/node": "^16.9.4",
        "autoprefixer": "^10.3.4",
        "browser-sync": "^2.27.5",
        "commitizen": "^4.2.4",
        "del": "^6.0.0",
        "esbuild": "^0.12.28",
        "esbuild-plugin-solid": "^0.4.0",
        "fibers": "^5.0.0",
        "gulp": "^4.0.2",
        "gulp-changed": "^4.0.3",
        "gulp-esbuild": "^0.8.7",
        "gulp-if": "^3.0.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^9.0.1",
        "gulp-pug": "^5.0.0",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.1.3",
        "gulp-sitemap": "^8.0.0",
        "gulp-size": "^4.0.1",
        "husky": "^7.0.2",
        "jstransformer-markdown-it": "^2.1.0",
        "postcss": "^8.3.6",
        "postcss-csso": "^5.0.1",
        "postcss-scss": "^4.0.0",
        "rollup": "^2.56.3",
