        "@semantic-release/git": "^10.0.1",
        "@types/bytes": "^3.1.1",
        "@types/d3": "^7.1.0",
        "@types/d3-array": "^3.0.2",
        "@types/d3-color": "^3.0.2",
        "@types/d3-hierarchy": "^3.0.2",
        "@types/d3-scale": "^4.0.2",
