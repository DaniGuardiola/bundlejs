{
  "name": "root",
  "version": "0.0.5",
  "private": true,
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "typedoc": "rm -r -f dists/docs/ && esbuild typedoc.tsx --jsx-factory=JSX.createElement --jsx-fragment=JSX.Fragment --format=cjs --outfile=typedoc.cjs && typedoc && node ./rehype.js",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "test": "pnpm build",
    "pre-release": "pnpm -r build && pnpm test",
    "semantic-release": "semantic-release",
    "repl": "tsm repl.ts"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            "lib/**"
          ]
        }
      ]
    ]
  },
  "changelog": {
    "repo": "bundlejs",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    },
    "cacheDir": ".changelog"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
    }
  },
  "dependencies": {
    "@bundlejs/core": "workspace:^0.0.0",
    "@dprint/formatter": "^0.2.0",
    "@okikio/animate": "^2.3.1",
    "@okikio/emitter": "^2.1.7",
    "@okikio/native": "^3.0.1",
    "@okikio/sharedworker": "^1.0.4",
    "@typescript/ata": "^0.9.3",
    "countapi-js": "^1.0.2",
    "monaco-editor": "^0.33.0",
    "typescript": "^4.7.2",
    "workbox-window": "^6.5.3"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^0.1.0",
    "@astrojs/solid-js": "^0.1.3",
    "@astrojs/tailwind": "^0.2.1",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@iconify/json": "^2.1.55",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "astro": "1.0.0-beta.38",
    "astro-compress": "^0.0.6",
    "astro-icon": "^0.7.1",
    "astro-service-worker": "^0.1.2",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "hastscript": "^7.0.2",
    "husky": "^8.0.1",
    "pnpm": "^7.1.7",
    "postcss-font-grabber": "^3.0.2",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^1.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "rehype-urls": "^1.1.1",
    "sass": "^1.52.1",
    "semantic-release": "^19.0.2",
    "solid-js": "^1.4.3",
    "tiny-glob": "^0.2.9",
    "tsm": "^2.2.1",
    "typedoc": "^0.22.17",
    "typedoc-plugin-extras": "^2.2.3",
    "typedoc-plugin-mdn-links": "^1.0.6",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "ultra-runner": "^3.10.5",
    "unified": "^10.1.2",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-icons": "^0.14.3",
    "vitest": "^0.13.1"
  }
}
