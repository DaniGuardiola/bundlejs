{
    "name": "bundlejs",
    "version": "0.0.5",
    "private": true,
    "license": "MIT",
    "type": "module",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "commit": "git cz",
        "build": "gulp build",
        "watch": "gulp --watch",
        "test": "pnpm build",
        "pre-release": "pnpm test",
        "semantic-release": "semantic-release",
        "repl": "tsm repl.ts"
    },
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/git",
            "@semantic-release/github"
        ]
    },
    "changelog": {
        "repo": "bundle",
        "labels": {
            "breaking": ":boom: Breaking Change",
            "enhancement": ":rocket: Enhancement",
            "bug": ":bug: Bug Fix",
            "documentation": ":memo: Documentation",
            "internal": ":house: Internal"
        },
        "cacheDir": ".changelog"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
        }
    },
    "author": {
        "name": "Okiki Ojo",
        "email": "hey@okikio.dev",
        "url": "https://okikio.dev"
    },
    "bugs": {
        "url": "https://github.com/okikio/bundle/issues"
    },
    "homepage": "https://bundlejs.com",
    "dependencies": {
        "@amoutonbrady/lz-string": "^0.0.1",
        "@dprint/formatter": "^0.2.0",
        "@okikio/animate": "beta",
        "@okikio/emitter": "beta",
        "@okikio/native": "^3.0.1",
        "@okikio/sharedworker": "^1.0.4",
        "@typescript/ata": "^0.9.3",
        "bytes": "^3.1.2",
        "countapi-js": "^1.0.2",
        "d3": "^7.6.1",
        "d3-array": "^3.2.0",
        "d3-color": "^3.1.0",
        "d3-hierarchy": "^3.1.2",
        "d3-scale": "^4.0.2",
        "d3-shape": "^3.1.0",
        "esbuild-wasm": "^0.14.51",
        "monaco-editor": "^0.33.0",
        "parse-package-name": "^1.0.0",
        "preact": "^10.10.0",
        "resolve.exports": "^1.1.0",
        "solid-js": "^1.4.8",
        "typescript": "^4.7.4",
        "web-animations-js": "^2.3.2",
        "webcola": "^3.4.0",
        "workbox-window": "^6.5.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
