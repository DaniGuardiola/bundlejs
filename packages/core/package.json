{
  "name": "@bundlejs/core",
  "version": "0.0.0",
  "access": "public",
  "type": "module",
  "description": "An animation library for the modern web which utilizes the Web Animation API (WAAPI) to create butter smooth animation.",
  "typedocMain": "src/index.ts",
  "main": "lib/index.js",
  "types": "@types/index.d.ts",
  "browser": "lib/index.mjs",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "default": "./lib/index.js"
    },
    "./lib/*": "./lib/*"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src",
    "types": "./@types"
  },
  "files": [
    "lib",
    "src",
    "@types"
  ],
  "scripts": {
    "build-wasm": "esno ./scripts/build-wasm.ts",
    "build": "vite build",
    "dev": "vite",
    "test:run": "vitest run && pnpm deno",
    "test": "vitest",
    "repl": "esno repl.ts",
    "deno": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run ./repl.ts",
    "dts": "del-cli @types/ && tsc --project dts.tsconfig.json",
    "pre-release": "pnpm build-wasm && pnpm build && pnpm test:run && pnpm dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikio/native.git"
  },
  "keywords": [
    "modern",
    "bundlejs",
    "bundle size checker",
    "esbuild-wasm",
    "gzip",
    "brotli",
    "deno",
    "bundler",
    "treeshake",
    "bundlephobia",
    "cdn",
    "github",
    "skypack",
    "unpkg",
    "compressed",
    "jsx",
    "minify",
    "typescript"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "bugs": {
    "url": "https://github.com/okikio/bundlejs/issues"
  },
  "homepage": "https://bundlejs.com",
  "dependencies": {
    "@amoutonbrady/lz-string": "^0.0.1",
    "@okikio/emitter": "^2.1.7",
    "bytes": "^3.1.2",
    "d3": "^7.4.4",
    "esbuild": "^0.14.45",
    "esbuild-wasm": "^0.14.45",
    "nanoid": "^4.0.0",
    "parse-package-name": "^1.0.0",
    "resolve.exports": "^1.1.0",
    "rollup": "^2.75.6",
    "solid-js": "^1.4.4",
    "webcola": "^3.4.0"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.0.0",
    "@types/webcola": "^3.2.0",
    "@vitest/ui": "^0.15.1",
    "del-cli": "^4.0.1",
    "esno": "^0.16.3",
    "typescript": "^4.7.3",
    "typescript-json-schema": "^0.53.1",
    "vite": "^2.9.12",
    "vitest": "^0.15.1"
  }
}