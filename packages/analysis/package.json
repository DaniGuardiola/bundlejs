{
  "name": "@bundlejs/analysis",
  "version": "0.0.0",
  "access": "public",
  "type": "module",
  "description": "An animation library for the modern web which utilizes the Web Animation API (WAAPI) to create butter smooth animation.",
  "typedocMain": "src/index.ts",
  "types": "@types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "default": "./lib/index.js"
    },
    "./lib/*": "./lib/*",
    "./types/*": "./@types/*",
    "./@types/*": "./@types/*",
    "./src/*": "./src/*"
  },
  "files": [
    "lib",
    "src",
    "@types"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "test:run": "vitest run && pnpm deno",
    "test": "vitest",
    "repl": "esno repl.ts",
    "deno": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run ./repl.ts",
    "dts": "del-cli @types/ && tsc --project dts.tsconfig.json",
    "verify": "pnpm test:run && pnpm dts",
    "pre-release": "ultra build && ultra verify"
  },
  "ultra": {
    "concurrent": [
      "verify",
      "test:run"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikio/bundlejs.git",
    "directory": "packages/analysis"
  },
  "keywords": [
    "modern",
    "bundlejs",
    "bundle size checker",
    "esbuild-wasm",
    "gzip",
    "brotli",
    "deno",
    "bundler",
    "treeshake",
    "bundlephobia",
    "cdn",
    "github",
    "skypack",
    "unpkg",
    "compressed",
    "jsx",
    "minify",
    "typescript",
    "bundle analysis",
    "package analyzer"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "bugs": {
    "url": "https://github.com/okikio/bundlejs/issues"
  },
  "homepage": "https://bundlejs.com",
  "dependencies": {
    "@bundlejs/core": "workspace:^0.0.0",
    "bytes": "^3.1.2",
    "d3": "^7.6.1",
    "esbuild-wasm": "^0.15.15",
    "nanoid": "^4.0.0",
    "solid-js": "^1.6.2",
    "webcola": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@types/bytes": "^3.1.1",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.11.9",
    "del-cli": "^5.0.0",
    "esno": "^0.16.3",
    "pnpm": "^7.17.0",
    "typescript": "^4.9.3",
    "ultra-runner": "^3.10.5",
    "vite": "^3.2.4",
    "vite-plugin-solid": "^2.4.0",
    "vitest": "^0.25.3"
  }
}