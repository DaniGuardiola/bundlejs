{
    "cleanUrls": true,
    "trailingSlash": true,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=63072000; includeSubDomains; preload"
                },
                {
                    "key": "Cache-Control",
                    "value": "max-age=480, must-revalidate, public"
                },
                {
                    "key": "Accept-CH",
                    "value": "DPR, Viewport-Width, Width"
                },
                {
                    "key": "X-UA-Compatible",
                    "value": "IE=edge"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'self'; font-src 'self' https://fonts.gstatic.com; style-src 'self' 'unsafe-inline'; img-src 'self' https://res.cloudinary.com https://api.producthunt.com https://opencollective.com data:; script-src 'self' https://opencollective.com https://*.bundlejs.com https://bundlejs.com https://cdn.polyfill.io 'unsafe-eval'; connect-src 'self' https:; block-all-mixed-content; upgrade-insecure-requests; base-uri 'self'; object-src 'none'; worker-src 'self'; manifest-src 'self'; media-src 'self' https://res.cloudinary.com; form-action 'self'; frame-src 'self' https://opencollective.com https://*.bundlejs.com https://bundlejs.com; frame-ancestors 'self' https:;"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "geolocation=(), microphone=(), usb=(), sync-xhr=(self), camera=(), interest-cohort=()"
                },
                {
                    "key": "Link",
                    "value": "<https://fonts.gstatic.com/s/manrope/v4/xn7gYHE41ni1AdIRggexSg.woff2>; rel=preload; as=font; type=font/woff2; crossorigin=anonymous, <https://fonts.gstatic.com/s/materialiconsround/v62/LDItaoyNOAY6Uewc665JcIzCKsKc_M9flwmP.woff2>; rel=preload; as=font; type=font/woff2; crossorigin=anonymous, </js/main.min.js>; rel=modulepreload; importance=high, </js/theme.min.js>; rel=modulepreload; importance=high"
                }
            ]
        },
        {
            "source": "/",
            "headers": [
                {
                    "key": "Link",
                    "value": "<https://fonts.gstatic.com/s/manrope/v4/xn7gYHE41ni1AdIRggexSg.woff2>; rel=preload; as=font; type=font/woff2; crossorigin=anonymous, <https://fonts.gstatic.com/s/materialiconsround/v62/LDItaoyNOAY6Uewc665JcIzCKsKc_M9flwmP.woff2>; rel=preload; as=font; type=font/woff2; crossorigin=anonymous, </js/theme.min.js>; rel=modulepreload; importance=high, </js/main.min.js>; rel=modulepreload; importance=high, </js/index.min.js>; rel=modulepreload, </js/codicon.ttf>; rel=preload; as=font; type=font/ttf; importance=low; crossorigin, </js/esbuild.worker.mjs>; rel=modulepreload; as=script; importance=high, </js/typescript.worker.js>; rel=prefetch"
                }
            ]
        },
        {
            "source": "/(.*).css",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=1209600, must-revalidate"
                },
                {
                    "key": "Content-Type",
                    "value": "text/css"
                }
            ]
        },
        {
            "source": "/(.*).ttf",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=1209600, must-revalidate"
                },
                {
                    "key": "Content-Type",
                    "value": "font/ttf"
                }
            ]
        },
        {
            "source": "/js/(.*).js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=1209600, must-revalidate"
                }
            ]
        },
        {
            "source": "/favicon/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=601209600, must-revalidate"
                }
            ]
        },
        {
            "source": "/(.*).svg",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=601209600, must-revalidate"
                }
            ]
        },
        {
            "source": "/js/sw.js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=1209600, must-revalidate"
                }
            ]
        },
        {
            "source": "/js/esbuild.wasm",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=1209600, must-revalidate"
                }
            ]
        },
        {
            "source": "/manifest.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=1209600, must-revalidate"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/measure/count.js", "destination": "https://analytics.bundlejs.com/umami.js" },
        { "source": "/measure/:measure*", "destination": "https://analytics.bundlejs.com/:measure" }
    ],
    "github": {
        "silent": true,
        "autoJobCancelation": true
    }
}